jobs:
- name: reboot-management-web-nodes
  plan:
  - get: aws-concourse
    trigger: false
  - .: (( inject meta.plan.create-aws-profiles ))
    config:
      params:
        AWS_ACC: "{{accounts['management']}}"
  - .: (( inject meta.plan.reboot-web-nodes ))
    config:
      params:
        AWS_ACC: "{{accounts['management']}}"
